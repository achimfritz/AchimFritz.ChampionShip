<![CDATA[{]]>
	"recentCup":<![CDATA[{]]>
		"name": "{recentCup.name}",
	      "links": [
	        <![CDATA[{]]>
	          "rel": "self",
	          "href": "{f:uri.action(arguments: '{cup: recentCup}', absolute: 1, controller: 'Cup', action: 'show', format: 'json')}"
	        }
	        ]
	  },
  "cups": [<f:for each="{cups}" as="cup" iteration="iteration">
    <![CDATA[{]]>
      "name": "{cup.name}",
      "links": [
        <![CDATA[{]]>
          "rel": "self",
          "href": "{f:uri.action(arguments: '{cup: cup}', absolute: 1, controller: 'Cup', action: 'show', format: 'json')}"
        }
      ]
    }{f:if(condition: iteration.isLast, else: ',')}</f:for>
  ]
}